{
    "name": "jiny/social",
    "description": "Social Authentication package for JinyPHP - OAuth, Social Login, and Social Media Integration",
    "keywords": [
        "jinyPHP",
        "framework",
        "auth",
        "social",
        "oauth",
        "login",
        "laravel"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/database": "^11.0",
        "socialiteproviders/manager": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Auth\\Social\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Auth\\Social\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Auth\\Social\\JinySocialServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}